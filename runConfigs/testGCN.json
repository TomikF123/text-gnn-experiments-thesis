{
  "experiment_name": "test",
  "run_name": "test_run",
  "dataset": {
      "name": "mr",
      "tvt_split": [0.9, 0, 0.1],
      "shuffle": true,
      "random_seed": 123,
      "vocab_size": 1000,
      "preprocess": {
        "remove_stopwords": true,
        "remove_rare_words": 5
      },
      "gnn_encoding": {
          "preset":"None",
          "window_size": 2,
          "x_type":"identity"
      }
      
  },
  
   "model_config":{
      "model_type":"text_gcn",
      "common_params": {
        "epochs": 15,
        "batch_size": 32,
        "seed": 123
      },
      "model_specific_params": {
        "hidden_dim": 128,
        "dropout": 0.5,
        "num_layers": 2,
        "lr": 0.001
      }
  
    }
  }
  